<h1>Lists#index</h1>
<p>Find me in app/views/lists/index.html.erb</p>

<ul>
  <% @lists.each do |list| %>

    <li><%= link_to list.name, list_path(list) %></li>
        <% end %>
</ul>


<div class="container-sm mt-5">
<h2>Create a new list</h2>
<%= simple_form_for(@list) do |f| %>
  <%= f.input :name %>
  <%= f.submit %>
<% end %>
</div>


