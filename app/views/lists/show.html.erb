<h1>Show list: <%= @list.name %></h1>





<%= link_to "index", lists_path %>


<%= link_to "Add a bookmark", list_path(@list) %>






<div class="container-sm mt-5">
<h2>Create a new bookmark</h2>
<%= simple_form_for(@list) do |f| %>
  <%= f.submit %>
<% end %>
</div>

<div class="coucou">
  <% @bookmarks.each do |bookmark| %>
  <%= bookmark.comment %>
  <%= bookmark.list_id %>
  <%= bookmark.movie_id %>
  <%= bookmark.id %>
  <% end %>
</div>
